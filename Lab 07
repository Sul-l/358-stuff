let sum = 0;
for (let i = 1; i <= 5; i++) {
    let mark = parseInt(prompt("Enter mark for subject " + i + ":"));
    sum += mark;
}
let average = sum / 5;
if (average >= 50) {
    console.log("Student passed. Average marks: " + average);
} else {
    console.log("Student failed. Average marks: " + average);
}
